                },
                {
                    "name": "LOG_LEVEL",
                    "value": "debug,info"
                },
                {
                    "name": "DB",
                    "value": "postgres12"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "POSTGRES_USER",
